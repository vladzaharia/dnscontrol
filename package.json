{
  "name": "dnscontrol-ts",
  "version": "1.0.0",
  "description": "Reproducible DNS record entries for zhr.one properties",
  "private": true,
  "scripts": {
    "postinstall": "./lib/download.sh",
    "build": "npm run lint && webpack",
    "preview-ci": "run-script-os",
    "preview-ci:darwin": "cd out && ../lib/dnscontrol-Darwin --diff2 preview",
    "preview-ci:linux": "cd out && ../lib/dnscontrol-Linux --diff2 preview",
    "preview-ci:win32": "cd out && ..\\lib\\dnscontrol --diff2 preview",
    "preview": "npm run build && npm run preview-ci",
    "push-ci": "run-script-os",
    "push-ci:darwin": "cd out && ../lib/dnscontrol-Darwin --diff2 push",
    "push-ci:linux": "cd out && ../lib/dnscontrol-Linux --diff2 push",
    "push-ci:win32": "cd out && ..\\lib\\dnscontrol --diff2 push",
    "push": "npm run build && npm run push-ci",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ext .ts .",
    "lint:prettier": "prettier --write ."
  },
  "author": "Vlad Zaharia",
  "license": "All Rights Reserved",
  "devDependencies": {
    "@types/node": "^13.7.7",
    "@types/webpack": "^4.41.7",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cli-confirm": "^1.0.1",
    "copy-webpack-plugin": "^6.1.1",
    "dotenv": "^16.0.0",
    "elliptic": ">=6.5.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.6.2",
    "run-script-os": "^1.1.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.9.10",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.9.2"
  }
}
